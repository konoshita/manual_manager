<% content_for(:title, t('.title')) %>
<h1><%= t('.title') %></h1>
<!-- 掲示板一覧 -->

<table>
    <tbody>
    <thead>
        <tr>
        <th><%= Manual.human_attribute_name(:id) %></th>
        <th><%= Manual.human_attribute_name(:title) %></th>
        <th><%= Manual.human_attribute_name(:user_id) %></th>
        <th><%= Manual.human_attribute_name(:created_at) %></th>
        <th></th>
        </tr>
      </thead>
    </tbody>
    <tbody>
        <% if @manuals.present? %>
          <%= render @manuals %>
        <% else %>
          <p><%= t('.no_result') %></p>
        <% end %>
        </tbody>
</table>