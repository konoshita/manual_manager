<tr>
  <td>
    <%= manual.id %>
  </td>
  <td>
    <%= manual.title %>
  </td>
  <td>
    <%= manual.user.name %>
  </td>
  <td>
    <%= manual.category.name %>
  </td>
  <td>
    <%= l manual.created_at, format: :short %>
  </td>
  <td>
    <%= link_to admin_manual_path(manual) do %>
      <i class="fa fa-plus"></i>
    <% end %>
  </td>
  <td>
    <%= link_to edit_admin_manual_path(manual) do%>
      <i class="fa fa-edit"></i>
      <% end %>
  </td>
  <td>
    <%= link_to  admin_manual_path(manual) , method: :delete, data: { confirm: "#{manual.title}を削除してもよろしいですか？" } do%>
      <i class="fa fa-trash"></i>
    <% end %>
  </td>
</tr>
