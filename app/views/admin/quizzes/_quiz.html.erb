<tr>
  <td>
    <%= quiz.id %>
  </td>
  <td>
    <%= quiz.title %>
  </td>
  <td>
    <%= l quiz.created_at, format: :short %>
  </td>
  <td>
    <%= link_to admin_quiz_path(quiz) do%>
      <i class="fa fa-plus"></i>
    <% end %>
  </td>
  <td>
    <%= link_to edit_admin_quiz_path(quiz) do%>
      <i class="fa fa-edit"></i>
    <% end %>
  </td>
  <td>
    <%= link_to admin_quiz_path(quiz) , method: :delete, data: { confirm: "#{quiz.title}を削除してもよろしいですか？" } do%>
      <i class="fa fa-trash"></i>
    <% end %>
  </td>
</tr>
